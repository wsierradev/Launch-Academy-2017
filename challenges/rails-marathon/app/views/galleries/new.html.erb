<% if flash[:notice] %>
  <%flash.each do |key, msg|%>
    <%= msg %>
  <% end %>
<% end%>
<%= form_for(@gallery) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>


  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>
